#########################################
# üåê PROFILE & MODE CONFIGURATION
----- Also u can edit this file , Also make sure to make this after the edit
----- from Example.env.example -> Profile.env    ; like this, cuz it is the format accepted.
#########################################

# [OPTIONAL] Sets the profile mode. Default: dev
PROFILE=dev

# [OPTIONAL] Turn on debug logs. "true" or "false"
DEBUG=false

# [OPTIONAL] Number of active chats the bot can handle
MAX_CHAT=5

# [OPTIONAL] Refresh time between scans (in seconds)
REFRESH_TIME=5

# [OPTIONAL] Delay in milliseconds to simulate human actions
SLOW_MO=250

# [OPTIONAL] Global mode for bot response outside admin control
GLOBAL_MODE=False

# [OPTIONAL] Restart delay (minutes)
RESTART_TIME=2


#########################################
# ü§ñ BOT CONFIGURATION ‚Äî üîê REQUIRED
#########################################

# ‚úÖ Your bot's WhatsApp number in international format (without '+')
BOT_NUMBER="9183XXXXXXXX"

# ‚úÖ Your bot's display name
BOT_NAME="Tweakio"

# ‚úÖ Country code (without '+' or leading zeros)
BOT_NUM_COUNTRY="91"

# ‚úÖ Admin WhatsApp number (who controls the bot)
ADMIN_NUMBER="9183XXXXXXXX"

# ‚úÖ Name for display/logging
ADMIN_NAME="Admin"


#########################################
# üîë API KEYS ‚Äî üîê REQUIRED FOR INTEGRATIONS
#########################################

# ‚úÖ Gemini (Google) API Key for AI responses
GEM_API_KEY="AIzaSy..."

# ‚úÖ GROQ or OpenRouter key for LLM support
GROQ_API_KEY="sk-..."

# ‚úÖ Agent.ai Key (if using agent mode features)
AGENT_AI_KEY="agent-key"

# ‚úÖ Agent ID and Webhook (optional, for agent.ai comms)
AGENT_ID=""
WEBHOOK_ID=""

# ‚úÖ Custom Search Engine ID (used in Google-like queries)
CSE_ID="your-cse-id"

# ‚úÖ Base URL for any backend integration
BASE_URL="https://api.yourserver.com"

# ‚úÖ Image link used in welcome or intro messages
INTRO_IMG_URL="https://imgur.com/xyz.jpg"


#########################################
# üîß SYSTEM TIMERS (ADVANCED USERS)
#########################################

# [OPTIONAL] Browser init timeout (ms)
BROWSER_INIT_TIMEOUT=10000

# [OPTIONAL] Login wait timeout (ms)
LOGIN_WAIT_TIME=180000

# [OPTIONAL] Login method: 1 = Scan QR | 2 = Code
LOGIN_METHOD=2


#########################################
# üõ† GITHUB ACTIONS SUPPORT (CI/CD)
#########################################

# ‚úÖ GitHub PAT token (keep this secret)
GH_TOKEN="ghp_..."

# ‚úÖ Repository name format: user/repo
REPO_NAME="yourusername/yourrepo"

# [OPTIONAL] Branch to push code or logs
BRANCH_NAME=main


#########################################
# üß† NATURAL LANGUAGE COMMANDS (ADVANCED)
#########################################

# [OPTIONAL] Used to separate commands in long AI outputs
QUANTIFIER="//"

# [OPTIONAL] Trigger prefix for NLP routing
NLP="/say"
